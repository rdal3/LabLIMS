# Environment variables for Lab-LIMS Backend
# Copy this file to .env and fill in the values

# REQUIRED - JWT Secret for token signing (minimum 32 characters)
JWT_SECRET=your-super-secret-key-minimum-32-characters-here

# Optional - CORS origin (default: http://localhost:5173)
CORS_ORIGIN=http://localhost:5173

# Optional - Server port (default: 3001)
PORT=3001
